# Podfile
workspace 'Runner.xcworkspace'

flutter_export_environment = File.expand_path('Flutter/flutter_export_environment.sh', __dir__)
if File.exist?(flutter_export_environment)
  puts "flutter_export_environment.sh trouv√©"
  system(". #{flutter_export_environment}")
else
  puts "flutter_export_environment.sh introuvable"
end

platform :ios, '13.0'

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  # Inclure le fichier podhelper.rb
  require File.expand_path('Flutter/podhelper.rb', __dir__)
  flutter_install_all_ios_pods(File.dirname(File.realpath(__FILE__)))

end








